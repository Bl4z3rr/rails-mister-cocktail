<h1>Cocktails#new</h1>

<div class="container">
  <div class="row">
    <div class="col-sm-6 offset-sm-3">
      <%= simple_form_for [@cocktail] do |f| %>
        <%= f.input :name %>
        <%= f.submit class: "btn btn-primary" %>
      <% end %>
      <br>
      <%= link_to "Back", cocktails_path, class: "btn btn-primary" %>
    </div>
  </div>
</div>
